# Alchemy API Keys (get from https://www.alchemy.com/)
# Replace with your actual API keys
NEXT_PUBLIC_ALCHEMY_API_KEY_1=your_first_alchemy_key_here
NEXT_PUBLIC_ALCHEMY_API_KEY_2=your_second_alchemy_key_here
NEXT_PUBLIC_ALCHEMY_API_KEY_3=your_third_alchemy_key_here
NEXT_PUBLIC_ALCHEMY_API_KEY_4=your_fourth_alchemy_key_here
NEXT_PUBLIC_ALCHEMY_API_KEY_5=your_fifth_alchemy_key_here

# WalletConnect Project ID (get from https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_walletconnect_project_id

# Security Configuration
NEXT_PUBLIC_SECURITY_ENABLED=true
NEXT_PUBLIC_RATE_LIMIT_ENABLED=true
NEXT_PUBLIC_CSRF_ENABLED=true

# Debug Mode (set to true for development)
NEXT_PUBLIC_DEBUG_SECURITY=false 